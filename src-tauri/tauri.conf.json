{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "workflow-form-tauri",
  "version": "1.0.0",
  "identifier": "com.workflowform",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "frontendDist": "../src"
  },
  "app": {
    "windows": [
      {
        "title": "Workflow Form - PowerAutomate Integration",
        "width": 1000,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.png"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "",
      "wix": null,
      "nsis": {
        "installerIcon": null,
        "installMode": "perMachine",
        "allowDowngrades": false,
        "installerSidebar": null,
        "displayLanguageSelector": false,
        "compression": "external",
        "shortcuts": true,
        "menu": true,
        "license": null,
        "warningsAsErrors": true,
        "customLanguageFiles": null,
        "runAfterFinish": true,
        "differentialPackage": false,
        "dmg": null,
        "guid": null,
        "artifactName": "${productName}_${version}_x64.exe"
      }
    },
    "macOS": {
      "frameworks": [],
      "minimumSystemVersion": "10.13",
      "exceptionDomain": "",
      "signingIdentity": null,
      "entitlements": null
    }
  },
  "plugins": {}
}

